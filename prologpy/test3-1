 
<query>
	<forloop index="0" rule="grand_parent ( X, Y )  :- parent_child ( X, Z ) , parent_child ( Z, Y ) ">
		<query>
			<forloop index="0" rule="grand_parent ( X, Y )  :- parent_child ( X, Z ) , parent_child ( Z, Y ) "></forloop>
			<forloop index="1" rule="parent_child ( alice, bob )  :- TRUE">
				<yield output="parent_child ( alice, bob ) " matchinghead="{Z: bob}"></yield>
				<query>
					<forloop index="0" rule="grand_parent ( X, Y )  :- parent_child ( X, Z ) , parent_child ( Z, Y ) "></forloop>
					<forloop index="1" rule="parent_child ( alice, bob )  :- TRUE"></forloop>
					<forloop index="2" rule="parent_child ( alice, bertie )  :- TRUE"></forloop>
					<forloop index="3" rule="parent_child ( charlie, daisy )  :- TRUE"></forloop>
					<forloop index="4" rule="parent_child ( bertie, chuck )  :- TRUE"></forloop>
					<forloop index="5" rule="parent_child ( bob, charlie )  :- TRUE">
						<yield output="parent_child ( bob, charlie ) " matchinghead="{X: charlie}"></yield>
						<yield output="grand_parent ( alice, charlie ) " matchinghead="{X: alice, Y: X}"></yield>
					</forloop>
					<forloop index="6" rule="parent_child ( chuck, david )  :- TRUE"></forloop>
				</query>
			</forloop>
			<forloop index="2" rule="parent_child ( alice, bertie )  :- TRUE">
				<yield output="parent_child ( alice, bertie ) " matchinghead="{Z: bertie}"></yield>
				<query>
					<forloop index="0" rule="grand_parent ( X, Y )  :- parent_child ( X, Z ) , parent_child ( Z, Y ) "></forloop>
					<forloop index="1" rule="parent_child ( alice, bob )  :- TRUE"></forloop>
					<forloop index="2" rule="parent_child ( alice, bertie )  :- TRUE"></forloop>
					<forloop index="3" rule="parent_child ( charlie, daisy )  :- TRUE"></forloop>
					<forloop index="4" rule="parent_child ( bertie, chuck )  :- TRUE">
						<yield output="parent_child ( bertie, chuck ) " matchinghead="{X: chuck}"></yield>
						<yield output="grand_parent ( alice, chuck ) " matchinghead="{X: alice, Y: X}"></yield>
					</forloop>
					<forloop index="5" rule="parent_child ( bob, charlie )  :- TRUE"></forloop>
					<forloop index="6" rule="parent_child ( chuck, david )  :- TRUE"></forloop>
				</query>
			</forloop>
			<forloop index="3" rule="parent_child ( charlie, daisy )  :- TRUE"></forloop>
